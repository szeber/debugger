{
    "name": "szemul/debugger",
    "description": "Debugger",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^9.5.5",
        "psr/http-message": "^1.0.1"
    },
    "suggest": {
        "psr/http-message": "To use the HTTP debug events"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\Debugger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\Debugger\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
